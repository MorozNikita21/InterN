import java.util.*;
public class Decimal implements Integer{
    int x;
    int y;
    Decimal(int x, int y){
        this.x = x;
        this.y = y;
    }
    public void summ(){
        int z;
        z = x + y;
        System.out.println(z);
    }
    public void diff(){
        int z;
        if (x > y) z = x - y;
        else z = y - x;
        System.out.println(z);
    }
    public void print(){
        System.out.println("Decimal");
    }
}
