public class Binary implements Integer{
    int x;
    int y;
    Binary(int x, int y){
        this.x = x;
        this.y = y;
    }
    public void summ(){
        int z;
        z = x + y;
        System.out.println(java.lang.Integer.toBinaryString(z));
    }
    public void diff(){
        int z;
        if (x > y) z = x - y;
        else z = y - x;
        System.out.println(java.lang.Integer.toBinaryString(z));
    }
    public void print(){
        System.out.println("Binary");
    }
}
